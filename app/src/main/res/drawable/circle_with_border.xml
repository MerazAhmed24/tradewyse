<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <size android:height="35dp" android:width="35dp"/>
            <padding android:top="7dp" android:bottom="7dp" android:right="7dp" android:left="7dp"/>
            <stroke
                android:width="1dp"
                android:color="#FFFF02"/>
        </shape>
    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <size android:height="27dp" android:width="27dp"/>
            <solid
                android:color="#FFFF02"/>
        </shape>
    </item>
</layer-list>